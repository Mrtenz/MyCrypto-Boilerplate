{
  "compilerOptions": {
    "baseUrl": "./src/",
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react",
    "allowJs": true,
    "noEmitOnError": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
